{
	"name": "hackaton",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "vite build;node deploy.cjs"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0",
		"@sveltejs/kit": "^1.10",
		"@typescript-eslint/eslint-plugin": "^5.54",
		"@typescript-eslint/parser": "^5.54",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.35",
		"eslint-config-prettier": "^8.6",
		"eslint-plugin-svelte3": "^4.0",
		"ftp-deploy": "^2.4.3",
		"postcss": "^8.4.21",
		"prettier": "^2.8",
		"prettier-plugin-svelte": "^2.9",
		"svelte": "^3.55",
		"svelte-check": "^3.0",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5",
		"typescript": "^4.9",
		"vite": "^4.1"
	},
	"type": "module",
	"dependencies": {
		"svelte-material-icons": "^3.0.4"
	}
}
